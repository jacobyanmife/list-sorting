def unique_list(L):
    x = []
    for a in L:
        if a not in x:
            x.append(a)
            x.sort()
    return x

print (unique_list([1,2,3,3,3,3,4,5]))
print (unique_list([1,2,3,3,3,3,4,5,3,3,3,3,4,5,6,6,11,22,33,44]))
print (unique_list([1,2,3,3,3,3,4,5,5,3,3,3,3,4,5,6,6,110,20,19,34]))
